.page-content {
    padding: 65px 20px 24px;
}


.container-fluid {
    padding-left: 0px;
    padding-right: 20px;
}


.ace-settings-container {
    top: 65px;
 }

 .sidebar:before {
    background-color: #efefef;
    border-right: 0px solid #CCCCCC;
    bottom: 0;
    content: "";
    display: block;
    position: fixed;
    top: 0;
    width: 189px;
    z-index: -1;
}
 
 
 
    
 
 h2 {
     
     font-size: 20px;
 }
 
 .navbar .navbar-inner {
    background: none repeat scroll 0 0 #efefef;
    border-bottom: 0px solid #CCCCCC;
 }
 
 
 .ace-nav > li.dark-beige {
    background: none repeat scroll 0 0 #bababa;
}
    
.user-info {
  max-width: 200px;
}  


div.logo{
    
    width: 190px;
    height: 60px;
    float : left;
    background-color: #efefef;
   
   
}

div.header-parkoil{
    
    height: 68px;
    float : right;
    line-height: 68px;
    font-size: 15px;
    padding-right: 10px;
     
}

.sidebar.fixed, .sidebar.sidebar-fixed {
    left: 0;
    position: fixed;
    top: 140px;
    z-index: 1029;
}

.navbar .brand {
    color: #FFFFFF;
    display: block;
    float: left;
    font-size: 20px;
    font-weight: 200;
    padding: 20px 20px;
    margin-left: 10px;
    height: 20px;
}   

.ace-nav > li {
    background: none repeat scroll 0 0 #2E6589;
    border-left: 1px solid #DDDDDD;
    line-height: 68px;
    max-height: 68px;
    padding: 0;
    position: relative;
}

@font-face {
  font-family: 'Open Sans';
  font-style: normal;
  font-weight: 300;
  src: local('Open Sans Light'), local('OpenSans-Light'), url(DXI1ORHCpsQm3Vp6mXoaTRsxEYwM7FgeyaSgU71cLG0.woff) format('woff');
